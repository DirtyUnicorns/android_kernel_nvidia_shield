#include "tegra210-jetson-tx1-p2597-2180-a01-android-devkit.dts"

/ {
	nvidia,dtsfilename = __FILE__;

	nvidia,boardids = "2530:0032:E0";
	nvidia,proc-boardid = "2530:0032:E0";
	nvidia,pmu-boardid = "2530:0032:E0";

	host1x {
		dc@54200000 {
			status = "okay";
			nvidia,dc-flags = <TEGRA_DC_FLAG_ENABLED>;
			nvidia,emc-clk-rate = <300000000>;
			nvidia,cmu-enable = <1>;
			nvidia,fb-bpp = <32>; /* bits per pixel */
			nvidia,fb-flags = <TEGRA_FB_FLIP_ON_PROBE>;
			nvidia,dc-or-node = "/host1x/sor1";
			avdd_hdmi-supply = <&max77620_ldo8>;
			avdd_hdmi_pll-supply = <&max77620_sd3>;
			vdd_hdmi_5v0-supply = <&vdd_hdmi>;
		};
		dc@54240000 {
			status = "disabled";
		};
	};
};
